{
  "expo": {
    "name": "Time Out: Mindful Life Tracker",
    "slug": "timeout",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon_final.png",
    "splash": {
      "image": "./assets/splash_image.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "nofussexe.timeout"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon_final.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nofussexe.timeout",
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./google-services.json",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ],
      "assetExts": [
        "png",
        "ttf"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "608ff4d1-617a-4afa-9fb8-15b0d81edb63"
      }
    }
  }
}